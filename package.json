{
  "name": "creditcardchecker-node",
  "version": "1.1.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production node js/app.js data/input_credit_cards_large.txt data/output_credit_cards.txt",
    "profile": "node --prof js/app.js data/input_credit_cards_large.txt data/output_credit_cards.txt",
    "profile-cc": "node --prof js/lib/credit_cards.profile.js",
    "debug": "DEBUG=*.ts node js/app.js",
    "compile": "npm run lint && rimraf js/* && tsc --sourcemap -p ts/",
    "lint": "tslint 'ts/**/*.ts'",
    "test": "npm run compile && mocha --require mocha-global.js $(find js/ -name *.spec.js)"
  },
  "author": "James Pepplinkhouse",
  "license": "MIT",
  "dependencies": {
    "ramda": "0.22.1",
    "worker-farm": "1.3.1"
  },
  "devDependencies": {
    "@types/chai": "3.4.34",
    "@types/mocha": "2.2.32",
    "@types/node": "6.0.41",
    "@types/ramda": "0.0.1",
    "chai": "3.5.0",
    "mocha": "3.1.0",
    "rimraf": "2.5.2",
    "source-map-support": "0.4.3",
    "tslint": "3.15.1",
    "typescript": "2.0.3"
  }
}
